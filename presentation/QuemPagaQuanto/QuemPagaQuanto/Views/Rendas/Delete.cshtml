@model QuemPagaQuanto.Models.Renda

@{
    ViewData["Title"] = "Delete";
}

<h1>Apagar renda</h1>

<h3>Tem certeza que deseja apagar esta renda?</h3>
<div>
    <h4>Renda</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Valor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Valor)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RendaFixa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RendaFixa)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Descricao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Data)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <a class="btn btn-info" asp-action="Index">Voltar</a> |
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
